<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <size
                android:width="108dp"
                android:height="108dp" />

            <stroke
                android:width="5dp"
                android:color="@color/line_color" />
        </shape>
    </item>

    <item
        android:bottom="30dp"
        android:left="30dp"
        android:right="30dp"
        android:top="30dp">
        <shape
            android:shape="rectangle"
            android:useLevel="true">
            <size android:height="48dp" android:width="48dp"/>
            <solid android:color="@color/line_color" />
        </shape>

    </item>
